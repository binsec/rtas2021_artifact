\mode<presentation>

%\setbeamercolor*{structure}{bg=gray,fg=black}

\definecolor{beamer@ceatitle}{rgb}{0.3,0.33,0.34}
\definecolor{beamer@ceawhite}{rgb}{.84,.84,.84}
\definecolor{beamer@ceawhitehigh}{rgb}{.94,.94,.94}
\definecolor{beamer@ceawhitelow}{rgb}{.74,.74,.74}
\definecolor{beamer@ceared}{rgb}{0.83,0.1,0.125}
\definecolor{beamer@ceagreen}{rgb}{.44,0.74,0.12}
\definecolor{beamer@cea1}{rgb}{0.83,0.1,0.125}
\definecolor{beamer@cea2}{rgb}{0.4,0.8,0.2}
\definecolor{beamer@cea3}{rgb}{0.83,0.1,0.125}
\definecolor{beamer@cea4}{rgb}{0.4,0.8,0.2}

\setbeamercolor*{palette primary}{use=structure,fg=white,bg=structure.fg}
\setbeamercolor*{palette secondary}{use=structure,fg=white,bg=structure.fg!75!black}
\setbeamercolor*{palette tertiary}{use=structure,fg=white,bg=structure.fg!50!black}
\setbeamercolor*{palette quaternary}{fg=white,bg=black}

\setbeamercolor*{sidebar}{use=structure,bg=structure.fg}
 
\setbeamercolor*{palette sidebar primary}{use=structure,fg=structure.fg!10}
\setbeamercolor*{palette sidebar secondary}{fg=white}
\setbeamercolor*{palette sidebar tertiary}{use=structure,fg=structure.fg!50}
\setbeamercolor*{palette sidebar quaternary}{fg=white}

\setbeamercolor*{titlelike}{parent=palette primary}

\setbeamercolor*{separation line}{}
\setbeamercolor*{fine separation line}{}

\setbeamercolor{section in toc}{fg=black, bg=beamer@ceawhite}
\setbeamercolor{section in toc shaded}{fg=red, bg=beamer@ceawhite}

\setbeamercolor{caption}{bg=beamer@ceawhite}
\setbeamercolor{caption name}{beamer@ceawhite}

\setbeamercolor{item}{fg=beamer@cea1, bg=white}
\setbeamercolor{subitem}{fg=beamer@cea2, bg=white}
\setbeamercolor{subsubitem}{fg=beamer@cea3, bg=white}

\setbeamercolor{block body}{bg=beamer@ceawhite}
\setbeamercolor{block body alerted}{bg=beamer@ceawhite}
\setbeamercolor{block body example}{bg=beamer@ceawhite}
\setbeamercolor{block title}{fg=structure.fg,bg=beamer@ceawhite}
\setbeamercolor{block title alerted}{fg=alerted text.fg,bg=beamer@ceawhitelow}
\setbeamercolor{block title example}{fg=example text.fg,bg=beamer@ceawhitehigh}

\setbeamercolor{title}{fg=structure.fg, bg=beamer@ceawhite}

\mode
<all>
